{
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/react-in-jsx-scope": "off",
		"react/prop-types": 1,
		"no-unused-vars": 1,
		"react/jsx-key": 1
	}
}
